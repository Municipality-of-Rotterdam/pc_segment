[run]
branch = True
source = src/pc_segment
data_file = ~/localfiles/.coverage_pc_segment

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:

ignore_errors = True
omit =
    tests/*
    setup.py
    # this file is autogenerated by dbx
    src/pc_segment/common.py
